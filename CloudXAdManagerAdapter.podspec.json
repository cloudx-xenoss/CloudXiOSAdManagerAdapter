{
  "name": "CloudXAdManagerAdapter",
  "version": "1.0.1",
  "summary": "Mobile SDK for CloudX iOS Ad Manager Adapter",
  "description": "iOS adapter add-on to the CloudX iOS SDK for Google's Ad Manager mediation.",
  "homepage": "https://github.com/cloudx-xenoss/CloudXiOSAdManagerAdapter",
  "authors": {
    "CloudX": "support@cloudx.com"
  },
  "license": {
    "type": "Proprietary",
    "text": "Copyright 2024 CloudX, Inc. All rights reserved."
  },
  "platforms": {
    "ios": "13.0"
  },
  "swift_versions": ["5.5", "5.6", "5.7", "5.8", "5.9", "5.10", "6.0"],
  "module_name": "CloudXAdManagerAdapter",
  "source": {
    "http": "https://github.com/cloudx-xenoss/CloudXiOSAdManagerAdapter/releases/download/1.0.1/CloudXAdManagerAdapter.xcframework.zip",
    "type": "zip",
    "flatten": false
  },
  "vendored_frameworks": "CloudXAdManagerAdapter.xcframework",
  "dependencies": {
    "CloudXCore": ">= 1.0.0",
    "Google-Mobile-Ads-SDK": "~> 10.0"
  },
  "frameworks": [
    "AVFoundation",
    "AVKit",
    "AdSupport",
    "CoreGraphics",
    "CoreLocation",
    "CoreTelephony",
    "Foundation",
    "StoreKit",
    "SystemConfiguration",
    "UIKit"
  ],
  "weak_frameworks": [
    "Combine",
    "CryptoKit",
    "SafariServices",
    "SwiftUI",
    "WebKit",
    "GoogleMobileAds"
  ],
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64",
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES",
    "DEFINES_MODULE": "YES",
    "SWIFT_VERSION": "5.0",
    "SWIFT_INSTALL_OBJC_HEADER": "NO",
    "SWIFT_OBJC_INTERFACE_HEADER_NAME": ""
  },
  "user_target_xcconfig": {
    "OTHER_LDFLAGS": "-ObjC",
    "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
  }
} 
